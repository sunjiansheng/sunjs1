       NVL(ROUND(B.ATTYPE_AMOUNT_OC / A.TYCBFZCJZ, 6), 0) ZHGBL,
       0 LCSBL,
       '''' BZ
       ,''1060000001'' AS PF_ID
  FROM (SELECT A.PF_ID,
               A.BIZ_DATE,
               B.CPDM,
               NVL(A.NV_COST,0)   AS     TYCBFZCJZ,
               NVL(A.NV_MV,0)   AS          YZDJZCJZ,
               NVL(A.NV_PLJE,0)  AS      YZDJZCJZPLJE,
               NVL(A.NV_PLD,0)    AS     YZDJZCJZPLD,
               NVL(A.NV_TZZHSYQX,0)   AS CPTZZHPJSYQX,
               NVL(A.NV_TZZHPJSYCXQ,0) AS CPTZZHPJSYCXQ
          FROM TC_PF_NAV_INFO A
          LEFT JOIN V_PF_CPDM B
            ON A.PF_ID = B.PF_ID
          LEFT JOIN V_PF_BSC_INFO_ATTACHED C
            ON B.TA_PF_ID = C.PF_ID
         WHERE A.BIZ_DATE = ''${FENDDATE}''--IN (SELECT BIZ_DATE FROM TT_DATE)
           AND C.CPLX = ''1''
           AND C.GZFF = ''1''
           AND A.PF_ID ${PFID}) A
  LEFT JOIN (SELECT NVL(SUM(ATTYPE_AMOUNT_OC), 0) ATTYPE_AMOUNT_OC,
                    PF_ID,
                    BIZ_DATE
               FROM TC_HP_PB_DOM_INFO
              WHERE BIZ_DATE = ''${FENDDATE}''--IN (SELECT BIZ_DATE FROM TT_DATE)
                AND C_DV_KM_CLS = ''KC_FZ''
                AND C_DAI_CODE = ''MCHGJRZC''
              GROUP BY PF_ID, BIZ_DATE) B
    ON A.PF_ID = B.PF_ID
   AND A.BIZ_DATE = B.BIZ_DATE
