values ('B1791EE1911B47B5B8FA1240E306F0AD', 'A00043', '基金经理信息', '
/*
1、?同一产品下设多个下属产品/份额类别时，按照产品主代码报送。
2、?首任基金经理/投资经理，其“任职日期”按照产品合同生效日期/产品成立日期填写，“离任日期”为根据公司决定确定的解聘日期；非首任基金经理/投资经理，“任职日期”和“离任日期”分别指根据公司决定确定的聘任日期和解聘日期。
3、?基金经理/投资经理任职日期/离任日期后5个交易日（含）内报送。
首次全量报送存续的产品，报送在任的基金经理/投资经理。

*/
SELECT FU_GET_FISP_SJZD(''报送机构编号'') JGBH,--机构编号
   SUBSTR(''${FENDDATE}'', 1, 4) BGND,--报告年度
   ''1'' BGQJ,--报告期间
   ''${FENDDATE}'' QCRQ,--期初日期
   ''${FENDDATE}'' QMRQ,--期末日期
   C.CPDM CPDM,--产品代码
   CARD_TYPE ZJLX,--证件类型
   CARD_NO ZJHM,--证件号码
   NAME XM,--姓名
   OFFICE_DATE RZRQ,--任职日期
   NVL(LEAVE_DATE,''-'') LRRQ,--离任日期
   NOTICE_DATE GGRQ,--公告日期
   REMARK BZ--备注
      ,C.PF_ID
    --,''1060000001'' as PF_ID
FROM TC_PF_BSC_INFO A
INNER JOIN  V_PF_CPDM C ON A.PF_ID = C.TA_PF_ID
INNER JOIN  TC_PF_FUNDMANAGER_INFO B ON C.CPDM = B.PF_ID
WHERE C.PF_ID IS NOT NULL
AND A.PF_SETUP_DATE <= ''${FENDDATE}''
AND A.PF_REAL_END_DATE >= ''${FENDDATE}''
AND (B.OFFICE_DATE = ''${FENDDATE}''  OR B.LEAVE_DATE = ''${FENDDATE}'')
ORDER BY C.CPDM, B.CARD_TYPE, B.CARD_NO, B.NAME, B.OFFICE_DATE, B.LEAVE_DATE                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                ', '0', null, 'A1004', null, 'zjh', '超级管理员', '25-5月 -19 04.10.25.628000 下午', null, null, 'A0000901', 'A0000202');

