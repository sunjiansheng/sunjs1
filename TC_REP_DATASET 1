       SUBSTR(''${FENDDATE}'', 0, 4) BGND,
       ''1'' BGQJ,
       ''${FENDDATE}'' QCRQ,
       ''${FENDDATE}'' QMRQ,
       A.CPDM,
       NVL(A.TYCBFZCJZ, 0) TYCBFZCJZ,
       NVL(A.YZDJZCJZ, 0) YZDJZCJZ,
       NVL(A.YZDJZCJZPLJE, 0) YZDJZCJZPLJE,
       NVL(A.YZDJZCJZPLD, 0) YZDJZCJZPLD,
       NVL(A.CPTZZHPJSYQX, 0) CPTZZHPJSYQX,
