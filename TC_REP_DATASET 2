                     WHEN T1.C_DV_ISSUE_MODE = ''WSS_WS_DZSX'' THEN
                      T1.ATTYPE_QTY
                   END) AS LTSXGFSL,
               SUM(CASE
                     WHEN T1.C_DV_ISSUE_MODE = ''SS'' AND T1.IS_STOP = ''0'' THEN
                      T1.ATTYPE_QTY
                   END) AS QTLTSXGFSL,
               SUM(CASE
                     WHEN (T1.C_DV_ISSUE_MODE LIKE ''WSS_WX%'' OR
                          T1.C_DV_ISSUE_MODE LIKE ''WSS_WS%'') AND
                          T1.C_DV_ISSUE_MODE NOT LIKE ''%ZF'' AND
