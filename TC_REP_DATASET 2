       NVL(T1.QTLTSXGFSL, 0) AS QTLTSXGFSL,
       NVL(T1.XFXGFSL, 0) AS XFXGFSL,
       NVL(T1.ZFGFSL, 0) AS ZFGFSL,
       NVL(T1.QMCB, 0) AS QMCB,
       NVL(T1.QMSZ, 0) AS QMSZ,
