                   SUM(ATTYPE_AMOUNT_OC) AS JE
                   ,SUM(ATTYPE_COST_OC) AS JE2
             FROM TC_HP_PB_DOM_INFO
            WHERE BIZ_DATE = ''${FENDDATE}''
