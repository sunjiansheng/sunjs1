                  NVL( FU_GET_ZJJRQ(T1.PF_ID, ''${FENDDATE}''),NVL(FU_GET_SGSHRQ(T2.CPDM,''${FENDDATE}''),''${FENDDATE}''))
       END AS SHKSRQ, --赎回开始日期
