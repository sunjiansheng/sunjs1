CASE WHEN SUM(NVL(RMB_ZZ,0)) = 0 THEN NULL ELSE SUM(NVL(RMB_ZZ,0)) END  as RMB_ZZ,--终止人民币
CASE WHEN SUM(NVL(WB_ZZ,0)) = 0 THEN NULL ELSE SUM(NVL(WB_ZZ,0)) END  as WB_ZZ,--终止外币
