                   WHEN ATTYPE_EXT_ID=  ''PB020101020000'' THEN ''PB020101020601''
                   -- 优先级 ->  住户
                   WHEN ATTYPE_EXT_ID = ''PB020201010000'' THEN ''PB020201010100''
                   -- 劣后级 ->  住户
                   WHEN ATTYPE_EXT_ID = ''PB020201020000'' THEN ''PB020201020100''
