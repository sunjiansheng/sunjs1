COMMENT ON COLUMN TC_PF_SHARECONV_INFO.SHARECONV_DATE
  IS ''份额折算日期'';
COMMENT ON COLUMN TC_PF_SHARECONV_INFO.SHARECONV_TYPE
  IS ''份额折算类型'';
COMMENT ON COLUMN TC_PF_SHARECONV_INFO.CREATE_PRSN
