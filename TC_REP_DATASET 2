                  T1.SYMBOL_CODE
               END AS ZQDM,
               MAX(T1.SEC_CODE_NAME) AS ZQMC,
               SUM(T1.ATTYPE_QTY) AS QMSL,
               SUM(CASE
                     WHEN T1.C_DV_ISSUE_MODE = ''WSS_WX_FGKXS'' THEN
                      T1.ATTYPE_QTY
