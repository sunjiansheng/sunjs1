            WHEN T3.CPLX = ''1'' THEN
             SUM(CASE
                   WHEN T4.EXCHANGEFLAG NOT IN (''0'', ''1'') THEN
                    T4.CONFIRMEDAMOUNT - NVL(T4.CHARGE, 0) + NVL(T4.INTEREST, 0)
                   ELSE
