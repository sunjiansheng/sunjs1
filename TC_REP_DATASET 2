              WHEN A.HGFX = ''HGJY_ZHG'' THEN ''负债债务工具''
              WHEN A.HGFX = ''HGJY_NHG'' THEN ''资产债务工具''
              WHEN A.HGFX = ''CK'' THEN ''资产债务工具'' 
            END NODE_NAME,
