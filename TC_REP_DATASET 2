CREATE TABLE TC_PF_SHARECONV_INFO
(
  ID             VARCHAR2(50) NOT NULL,
  PF_ID          VARCHAR2(32) NOT NULL,
  PF_TYPE        VARCHAR2(8),
  SHARECONV_DATE VARCHAR2(8) NOT NULL,
  SHARECONV_TYPE VARCHAR2(8) NOT NULL,
  CREATE_PRSN    VARCHAR2(20),
  CREATE_TIME    DATE,
  MDFY_PRSON     VARCHAR2(20),
  MDFY_TIME      DATE
)
