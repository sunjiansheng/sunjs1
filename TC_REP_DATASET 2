CASE WHEN SUM(NVL(WB_XC,0)) = 0 THEN NULL ELSE SUM(NVL(WB_XC,0)) END as WB_XC,--续存外币
CASE WHEN SUM(NVL(RMB_XC,0)+NVL(WB_XC,0)) = 0 THEN NULL ELSE SUM(NVL(RMB_XC,0)+NVL(WB_XC,0)) END  as HJ_XC,--续存合计
