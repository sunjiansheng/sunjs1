                     WHEN SUBSTR(SUBJ_ID, 1, 4) IN (''1203'', ''1204'') THEN
                      GET_CODE(''C_MKT_CODE'', MARKET_CODE)
                     ELSE
                      ''199''
                   END AS JYCSDM,
