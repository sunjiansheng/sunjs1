   CASE WHEN D.CPLX = ''1'' THEN B.LIST_MARKETPLACE END SSJYCS,--上市交易场所
   CASE WHEN D.CPLX = ''1'' THEN B.IS_INLANDMUT_FUNDSHARE END SFNDHRJJFE,--是否内地互认基金分
   CASE WHEN D.CPLX = ''1'' THEN B.EXP_ANNUAL_RETURN_RATE  END YQNSYL,--预期年收益
