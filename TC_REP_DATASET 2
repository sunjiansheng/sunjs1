         WHEN T1.FUND_TYPE = ''9'' OR T3.SFNDHRJJ = ''1'' THEN
          0
         ELSE
          NVL(COUNT(DISTINCT T5.CERTIFICATENO || T5.CERTIFICATETYPE ||
                    T5.INVESTORNAME),
              0)
       END AS YXRGRS, --有效认购人数
