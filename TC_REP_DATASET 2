SELECT FU_GET_FISP_SJZD(''报送机构编号'') JGBH,
       SUBSTR(''${FENDDATE}'', 0, 4) AS BGND,
       ''1'' AS BGQJ,
       ''${FENDDATE}'' AS QCRQ,
       ''${FENDDATE}'' AS QMRQ,
       NVL(T4.CPDM, '''') AS CPDM,
       NVL(T1.JYCSDM, '''') AS JYCSDM,
       NVL(T1.ZQDM, '''') AS ZQDM,
       NVL(T1.ZQMC, '''') AS ZQMC,
